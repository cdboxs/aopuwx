/* pages/c_map/index.wxss */
.signMap {
  width: 100%;
}

.signMap .mapTip {
  width: 100%;
  height: 110rpx;
  padding: 0 0 0 20rpx;
  background: #fff;
  display: flex;
  font-size: 26rpx;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
  border-bottom: 1rpx solid #ddd;
}

.signMap .mapTip cover-view:nth-of-type(2) {
  color: red;
}

