<!--pages/member_modular/setMyInfo/index.wxml-->
<form bindsubmit='setMyInfo' class='setMyInfo'>
  <input type='text' name="userName" value='{{userInfo.userName}}' placeholder='请输入用户名'></input>
  <input type='number' name="phoneNum" disabled="true" value='{{userInfo.phone}}'></input>
  <button form-type='submit'>保存</button>
</form>
