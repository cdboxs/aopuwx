/* pages/member_modular/setMyInfo/index.wxss */
.setMyInfo{
  width:90%;
  display: block;
  margin:50rpx auto;
}
.setMyInfo input{
  display: block;
  height: 80rpx;
  margin:30rpx 0;
  padding: 0 20rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  border:1rpx solid #ddd;
}
.setMyInfo button{
  width:100%;
  height: 80rpx;
  color: #fff;
  line-height: 80rpx;
  border-radius: 0;
  background:#2cabe2;
}
















