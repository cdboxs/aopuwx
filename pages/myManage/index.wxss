/* pages/myManage/index.wxss */

.myManage {
  width: 100%;
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
}

.myManage .search {
  width: 94%;
  height: 60rpx;
  border-radius: 50rpx;
  margin: 20rpx auto;
  border: 1rpx solid #ddd;
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  position: relative;
}

.myManage .search input {
  width: 100%;
  height: 60rpx;
  color: #6b6b6b;
  padding: 0 20rpx;
  font-size: 30rpx;
  box-sizing: border-box;
}

.myManage .search .iconfont {
  font-size: 40rpx;
  color: #999;
}

.myManage .managelist {
  width: 100%;
  height: 200rpx;
  margin: 0 0 25rpx 0;
  border-left: 5px solid #c8b81a;
  box-sizing: border-box;
  box-shadow: 0rpx 7rpx 10rpx 5rpx #e5e5e5;
}

.myManage .managelist view {
  width: 100%;
  height: 100rpx;
  color: #666;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 32rpx;
}

.myManage .managelist view:nth-of-type(1) {
  border-bottom: 1px solid #ddd;
}

.myManage .managelist view text {
  width: 180rpx;
  color: #9e9e9e;
  padding: 0 0 0 20rpx;
}
.searchResult{
  width:95%;
  padding: 0 20rpx;
  background:#fefefe;
  box-sizing: border-box;
  position: absolute;top:58rpx;left:2.5%;z-index: 999999;
  border:1px solid #ddd;   
  border-top:0;
  
}
.searchResult view{
  padding:10rpx 0;
  display: flex;
  flex-flow: row wrap;
  border-bottom:1px solid #eee;
}
.searchResult text{
  display: block;
  width:100%;
  color:#666;
  font-size: 30rpx;
  padding:5rpx 0;
}
.searchResult text:nth-of-type(2){
  font-size: 25rpx;
  color:#999;
}
